<html>
  <body>
    <p id="main">default</p>
  </body>
  <script>
    console.log("top");
    xmlhttp=new XMLHttpRequest();
    xmlhttp.open("GET","http://localhost:8000/txs", false);
    xmlhttp.send();
    xmlDoc=xmlhttp.responseText;
    console.log(xmlDoc);
    document.getElementById("main").innerHTML = xmlDoc;
  </script>
